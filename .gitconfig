[core]
    excludesfile = /Users/UnnamE/.gitignore_global
    editor = vim
    ignorecase = false
	;pager = less -+X
[filter "lfs"]
    clean = git-lfs clean %f
    smudge = git-lfs smudge %f
    required = true
[merge]
    tool = vimdiff
[alias]
    list = log --abbrev-commit --graph --decorate --pretty=oneline
    follow = !git --no-pager log --follow --oneline
[color "status"]
    added = green bold
    changed = yellow bold
    untracked = red bold
[pager]
	branch = false
